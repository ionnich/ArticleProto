{
    "manifest_version": 2,
    "name": "Article Manager",
    "version": "1.0",
    "description": "A chrome extension to manage articles",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": ["tabs", "activeTab", "storage", "downloads"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  